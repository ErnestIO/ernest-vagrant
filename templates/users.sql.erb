CREATE TABLE users (
    id integer NOT NULL PRIMARY KEY,
    group_id integer,
    username text,
    password text,
    email text,
    salt text,
    admin boolean,
    created_at timestamp with time zone,
    updated_at timestamp with time zone,
    deleted_at timestamp with time zone
);
